citHeader("To cite package 'nycdogs' in publications use:")
     
 year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", meta$Date, perl = TRUE)
 vers <- paste("R package version", meta$Version)
 
 citEntry(entry="Manual",
		  title = "nycdogs: Dogs of New York",
		  author = personList(as.person("Kieran Healy")),
		  year = year,
		  note = vers,
		  textVersion =
		  paste("Kieran Healy (",
				year,
				"). nycdogs: Dogs of New York. ",
				vers, ".", sep=""))

    bibtype  = "Manual",
    title = "nycdogs: Dogs of New York",
    author   = person(given = "Kieran",
                      family = "Healy",
                      role = c("aut", "cre"),
                      email = "kjhealy@soc.duke.edu"),
    year = "2019",
    note = "R package version 0.1.0",
    url      = "http://kjhealy.github.io/nycdogs",
  key      = "nycdogs")
